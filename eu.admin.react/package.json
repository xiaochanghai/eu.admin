    "@types/lodash": "^4.17.14",
    "@types/react-custom-scrollbars": "^4.0.13",
    "ahooks": "^3.8.4",
    "antd": "^5.23.2",
    "antd-img-crop": "^4.24.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "echarts-liquidfill": "^3.1.0",
    "i18next": "^23.16.8",
    "keepalive-for-react": "^2.0.19",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-ace": "^13.0.0",
    "react-colorful": "^5.6.1",
