{
  "name": "eu-admin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "eu-admin open source management system",
  "author": {
    "name": "xiaochanghai",
    "email": "xiaochanghai@foxmail.com",
    "url": "https://github.com/xiaochanghai"
  },
  "license": "MIT",
  "homepage": "https://github.com/xiaochanghai/eu-Admin",
  "repository": {
    "type": "git",
    "url": "git@github.com:xiaochanghai/eu-Admin.git"
  },
  "bugs": {
    "url": "https://github.com/xiaochanghai/eu-Admin/issues"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build:dev": "tsc && vite build --mode development",
    "build:test": "tsc && vite build --mode test",
    "build": "tsc && vite build --mode production",
    "type:check": "tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    "@ant-design/happy-work-theme": "^1.0.0",
