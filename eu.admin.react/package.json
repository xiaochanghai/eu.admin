    "less": "^4.2.2",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.14.1",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.7.3",
